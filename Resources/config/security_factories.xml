<?xml version="1.0" ?>

<container xmlns="http://symfonyadsfaafsd.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="opensky_ldap.authentication.factory.ldap_auth.class">OpenSky\Bundle\LdapBundle\DependencyInjection\Security\LdapAuthenticationFactory</parameter>
    </parameters>

    <services>
        <service id="opensky_ldap.authentication.factory.ldap_auth" class="%opensky_ldap.authentication.factory.ldap_auth.class%">
            <tag name="security.listener.factory" />
        </service>
    </services>
</container>
